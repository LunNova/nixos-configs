---
 nixos/modules/module-list.nix                 |  1 +
 .../x11/display-managers/cosmic-greeter.nix   | 52 +++++++++++++++++++
 2 files changed, 53 insertions(+)
 create mode 100644 nixos/modules/services/x11/display-managers/cosmic-greeter.nix

diff --git a/nixos/modules/module-list.nix b/nixos/modules/module-list.nix
index af26e31b1cbf19..51112b27407ced 100644
--- a/nixos/modules/module-list.nix
+++ b/nixos/modules/module-list.nix
@@ -1407,6 +1407,7 @@
   ./services/x11/colord.nix
   ./services/x11/desktop-managers/default.nix
   ./services/x11/display-managers/default.nix
+  ./services/x11/display-managers/cosmic-greeter.nix
   ./services/x11/display-managers/gdm.nix
   ./services/x11/display-managers/lightdm.nix
   ./services/x11/display-managers/sddm.nix
